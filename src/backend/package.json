    "@jest/types": "^29.x",
    "@types/bcrypt": "^5.0.x",
    "@types/jest": "^29.x",
    "@types/jsonwebtoken": "^9.0.x",
    "@types/supertest": "^2.0.x",
    "@typescript-eslint/eslint-plugin": "^5.62.x",
    "@typescript-eslint/parser": "^5.62.x",
    "eslint": "^8.47.x",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.x",
